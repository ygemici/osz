#device       	mountpoint	opt/access	passno
/dev/mem     	/        	ro		1
/dev/tmp      	/tmp     	rw		1
/dev/vol/esp  	/boot    	ro,wheel:rw	2
/dev/vol/home 	/home    	users:rw	2
/dev/vol/usr  	/usr     	ro,wheel:rw	2
/dev/vol/var  	/var     	rw		2

# auto mount (not mounted on boot)
/dev/disk1   	/mnt     	rw		0
/dev/disk2   	/mnt     	rw		0
/dev/cdrom0  	/mnt     	ro		0
/dev/usbdisk0	/mnt     	rw		0
/dev/usbdisk1	/mnt     	rw		0
/dev/usbdisk2	/mnt     	rw		0
